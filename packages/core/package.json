{
  "name": "core",
  "type": "module",
  "version": "0.0.0",
  "exports": {
    ".": "./dist/index.js",
    "./adapters/vite": "./dist/adapters/vite.js",
    "./adapters/webpack": "./dist/adapters/webpack.js",
    "./api": "./dist/api.js",
    "./core/options": "./dist/core/options.js",
    "./types": "./dist/types/index.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.ts",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "unplugin": "^3.0.0"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.6.0",
    "@sxzz/prettier-config": "^2.3.1",
    "@sxzz/test-utils": "^0.5.15",
    "@types/node": "^25.2.0",
    "bumpp": "^10.4.0",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "tsdown": "^0.20.1",
    "tsdown-preset-sxzz": "^0.3.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rspack",
    "rollup",
    "rolldown",
    "esbuild",
    "farm"
  ]
}